        <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="220.0" />
        <Button defaultButton="true" mnemonicParsing="false" onAction="#handleContinueAction" prefWidth="80.0" text="Continuer">
