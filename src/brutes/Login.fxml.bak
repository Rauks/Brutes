<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" onKeyPressed="handleKeyPressed" prefHeight="350.0" prefWidth="500.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="LoginController">
  <children>
    <VBox spacing="20.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
      <children>
        <VBox VBox.margin="$x1">
          <children>
            <Label text="Login">
              <labelFor>
                <TextField fx:id="login">
                  <VBox.margin>
                    <Insets fx:id="x1" />
                  </VBox.margin>
                </TextField>
              </labelFor>
            </Label>
            <fx:reference source="login" />
          </children>
        </VBox>
        <VBox>
          <children>
            <Label text="Password">
              <labelFor>
                <PasswordField fx:id="password" />
              </labelFor>
            </Label>
            <fx:reference source="password" />
          </children>
        </VBox>
        <HBox alignment="TOP_CENTER">
          <children>
            <StackPane>
              <children>
                <ProgressIndicator fx:id="loading" prefHeight="22.0" prefWidth="22.0" translateX="60.0" visible="false" />
                <Button fx:id="connexion" mnemonicParsing="false" onAction="handleButtonAction" text="Connexion" />
              </children>
            </StackPane>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@style.css" />
  </stylesheets>
</AnchorPane>
